<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="com.collins.trustedmethods.matrics.analysis.handlers.SoarTranslatorHandler"
            commandId="com.collins.trustedmethods.matrics.analysis.commands.soartranslator">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            categoryId="com.collins.trustedmethods.matrics.analysis.commands"
            id="com.collins.trustedmethods.matrics.analysis.commands.soartranslator"
            name="Soar Translator">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="menu:org.osate.ui.analysesMenu?after=additions">
         <command
               commandId="com.collins.trustedmethods.matrics.analysis.commands.soartranslator"
               id="com.collins.trustedmethods.matrics.analysis.menus.soartranslator"
               style="push">
         </command>
      </menuContribution>
   </extension>

